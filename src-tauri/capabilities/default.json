{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "fs:default",
    "dialog:default",
    "notification:default",
    "process:default",
    "updater:default",
    "updater:allow-check",
    "updater:allow-download-and-install",
    "global-shortcut:default"
  ]
}
